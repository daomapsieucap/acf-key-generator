{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.4.0"
    }
  },
  "routes": [
    { "src": "/",  "dest": "/api/index.php" },
    { "src": "/(css)/(.*)",  "dest": "/css/$1" },
    { "src": "/(js)/(.*)",  "dest": "/js/$1" },
    { "src": "/(images)/(.*)",  "dest": "/images/$1" }
  ]
}